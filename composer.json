{
  "name": "memdufaizan/filament-table-columns-persist",
  "description": "Persist Filament table column visibility per user with defaults.",
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Memdufaizan\\FilamentTableColumnsPersist\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Memdufaizan\\FilamentTableColumnsPersist\\ColumnPrefsServiceProvider"
      ],
      "aliases": {
        "ColumnPrefs": "Memdufaizan\\FilamentTableColumnsPersist\\Facades\\ColumnPrefs"
      }
    }
  },
  "require": {
    "php": ">=8.1",
    "illuminate/support": "^10.0|^11.0",
    "filament/tables": "^3.0"
  }
}